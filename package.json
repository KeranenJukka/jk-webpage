    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "docker-compose -f docker-compose-dev.yml up",
    "prod": "docker-compose -f docker-compose-prod.yml up",
    "heroku": "heroku container:push web --app jukka-keranen && heroku container:release web --app jukka-keranen"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@react-three/test-renderer": "^10.0.3"
  }
}
